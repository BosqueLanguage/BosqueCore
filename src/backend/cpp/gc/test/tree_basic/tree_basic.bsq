datatype BasicTree of 
Leaf { v: Nat }
| Node { v: Nat, l: BasicTree, r: BasicTree }
;

function makeTree(depth: Nat, val: Nat): BasicTree {
    if(depth == 0n) {
        return Leaf { val };
    }
    
    return Node {
        val,
        makeTree(depth - 1n, val + 1n),
        makeTree(depth - 1n, val + 1n)
    };
}

public function main(): Bool {
    let t = makeTree(3n, 3n);
    return true;
}